{
  "icon": "/assets/home_mindtrails.png",
  "home": {
    "title": "MindTrails",
    "element": {
      "type": "list",
      "elements": [
        {
          "type": "goals"
        },
        {
          "type": "sessions",
          "left": {
            "text": "{0} Sessions Completed",
            "icon": {
              "url": "/assets/completed.png",
              "tint": false
            }
          },
          "right": {
            "text": "Launch Session",
            "icon": {
              "url": "/assets/unlocked.png",
              "tint": false
            },
            "action": "flow://flows/doses"
          }
        },
        {
          "type": "button",
          "action": {
            "text": "Show Survey Modal",
            "action": "navmodal://Survey"
          }
        },
        {
          "type": "carousel",
          "actions": [
            {
              "text": "inputs!",
              "icon": "/assets/home_mindtrails.png",
              "action": "flow://flows/inputs",
              "backgroundcolor": "#1A206AFF",
              "markcompleted": true
            },
            {
              "text": "survey 1",
              "icon": "/assets/home_instructions.png",
              "action": "flow://flows/how%20was%20your%20day%3f",
              "backgroundcolor": "#1A00E05A",
              "markcompleted": true
            },
            {
              "text": "survey 2",
              "icon": "/assets/home_resources.png",
              "action": "flow://flows/survey2",
              "backgroundcolor": "#1A00C2FF",
              "markcompleted": true
            },
            {
              "text": "survey 3",
              "icon": "/assets/home_anxiety.png",
              "action": "flow://flows/survey3",
              "backgroundcolor": "#1A5C2FDA",
              "markcompleted": true
            }
          ]
        },
        {
          "type": "tiles",
          "actions": [
            {
              "text": "inputs!",
              "icon": "/assets/home_mindtrails.png",
              "action": "flow://flows/inputs",
              "backgroundcolor": "#1A206AFF",
              "markcompleted": true
            },
            {
              "text": "survey 1",
              "icon": "/assets/home_instructions.png",
              "action": "flow://flows/how%20was%20your%20day%3F",
              "backgroundcolor": "#1A00E05A",
              "markcompleted": true
            },
            {
              "text": "survey 2",
              "icon": "/assets/home_resources.png",
              "action": "flow://flows/survey2",
              "backgroundcolor": "#1A00C2FF",
              "markcompleted": true
            },
            {
              "text": "survey 3",
              "icon": "/assets/home_anxiety.png",
              "action": "flow://flows/survey3",
              "backgroundcolor": "#1A5C2FDA",
              "markcompleted": true
            }
          ]
        }
      ]
    }
  },
  "menu": [
    {
      "text": "Home",
      "icon": "/assets/menu_home.png"
    }
  ],
  "settings": {
    "configs": "flow://flows/configs"
  },
  "triggers": [
    {
      "type": "timing",
      "action": "notification",
      "content": {
        "path": "flow://flows/survey1"
      },
      "frequency": "00:05:00"
    },
    {
      "type": "timing",
      "action": "notification",
      "frequency": "1",
      "content": {
        "time": "08:00",
        "path": "flow://flows/survey2",
        "expiry": "11:00"
      }
    },
    {
      "type": "random",
      "days": 7,
      "action": "notification",
      "actions": [
        {
          "default_time": "14:00",
          "time": "${random1_time}",
          "path": "flow://flows/random1",
          "expiry": "11:00"
        },
        {
          "default_time": "15:00",
          "time": "${random2_time}",
          "path": "flow://flows/random2",
          "expiry": "11:00"
        },
        {
          "default_time": "15:00",
          "time": "${random3_time}",
          "path": "flow://flows/random3"
        }
      ]
    }
  ],
  "probes": [
    {
      "type": "Location",
      "optional": true,
      "interval": "00:00:01",
      "accuracy": 5
    }
  ],
  "devices": [
    {
      "type": "Oura",
      "channel": "Api",
      "path": "https://api.ouraring.com/v2/usercollection",
      "androidsync": "deeplink://com.ouraring.oura",
      "iossync": "deeplink://Oura://",
      "endpoints": [
        {
          "path": "sleep",
          "days": 1,
          "writesdatum": true
        },
        {
          "path": "workout",
          "days": 1,
          "writesdatum": true
        },
        {
          "path": "daily_activity",
          "days": 10,
          "writesdatum": true,
          "goals": [
            {
              "type": "percentile",
              "show-loading": true,
              "field": "active",
              "days": 10,
              "percentile": 0.6
            }
          ],
          "triggers": [
            {
              "prompt": "Hi! We’ve noticed your tracker hasn’t recorded much data over the past two days. Please try to wear it as much as possible—ideally all day. If it’s not working or you’re having trouble, let us know so we can help.",
              "timewindowdays": 1,
              "aggregation": "sum",
              "condition": [
                "non_wear_time",
                ">",
                "36000"
              ]
            },
            {
              "prompt": "Hi! Your step count is low, make sure you to walk and stay active.",
              "timewindowdays": 1,
              "aggregation": "sum",
              "condition": [
                "steps",
                "<",
                "2000"
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "WisePill",
      "channel": "Webhook",
      "goals": [
        {
          "type": "binarypercentage",
          "field": "event",
          "days": 7,
          "bounds": [
            {
              "lower-bound": 0,
              "upper-bound": 0.6,
              "text": "You're missing several doses. Please try to take your medication more consistently.",
              "summary": "Low Adherence",
              "color": "#ff9187"
            },
            {
              "lower-bound": 0.6,
              "upper-bound": 0.8,
              "text": "You're taking most of your doses. A little more consistency will help.",
              "summary": "Moderate Adherence",
              "color": "#F5E050"
            },
            {
              "lower-bound": 0.8,
              "upper-bound": 1,
              "text": "You're consistently taking your medication. Keep it up!",
              "summary": "High Adherence",
              "color": "#65bf65"
            }
          ]
        }
      ]
    }
  ]
}